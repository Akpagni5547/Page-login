{
    "name": "biomedicale",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "nx": "nx",
        "------RUN-ALL-MICRO-FRONTENDS-TOGATHER-CMDs(this will allow us to run all apps togather)------": "",
        "dev": "concurrently \"yarn root:app:dev\"",
        "---------- DEVELOPMENT CMDs(add all microfrontend dev commands here)--------------": "",
        "root:app:dev": "nx serve root --port=3000",
        "---------- PRODUCTION CMDs ----------------------------------------------------------------------------------": "",
        "build:root:app": "nx build root --prod",
        "---------- TESTING CMDs -------------------------------------------------------------------------------------": "",
        "ts": "tsc --noEmit",
        "test": "nx test",
        "lint": "nx workspace-lint && nx lint --verbose",
        "e2e": "nx e2e",
        "ts:watch": "npm run ts -- --watch",
        "---------- ALL ABOUT COMMIT CMDs ---------------": "",
        "commit": "git cz",
        "lint:fix": "nx lint --fix",
        "format:fix": "precise-commits",
        "commitmsg": "validate-commit-msg",
        "precommit-lint": "run-s format:fix commitmsg",
        "release": "standard-version",
        "------ALL-CMSs-RELATIVES-TO-NX------": "",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "update": "nx migrate latest",
        "workspace-generator": "nx workspace-generator",
        "dep-graph": "nx dep-graph",
        "help": "nx help"
    },
    "private": true,
    "dependencies": {
        "clsx": "^1.1.1",
        "concurrently": "^6.2.0",
        "connected-react-router": "^6.8.0",
        "core-js": "^3.6.5",
        "dayjs": "^1.8.28",
        "document-register-element": "1.13.1",
        "formik": "^2.1.4",
        "history": "^4.10.1",
        "lodash": "^4.17.15",
        "lodash.groupby": "^4.6.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.2.0",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-freeze": "^0.1.7",
        "redux-thunk": "^2.3.0",
        "regenerator-runtime": "0.13.7",
        "reselect": "^4.0.0",
        "tslib": "^2.0.0"
    },
    "devDependencies": {
        "@babel/core": "7.12.13",
        "@babel/preset-env": "7.12.13",
        "@babel/preset-react": "7.12.13",
        "@babel/preset-typescript": "7.12.13",
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-angular": "8.3.4",
        "@commitlint/config-conventional": "8.3.4",
        "@craco/craco": "^5.6.4",
        "@nrwl/cli": "12.5.7",
        "@nrwl/cypress": "12.5.7",
        "@nrwl/eslint-plugin-nx": "12.5.7",
        "@nrwl/jest": "12.5.7",
        "@nrwl/linter": "12.5.7",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/react": "12.5.7",
        "@nrwl/tao": "12.5.7",
        "@nrwl/web": "12.5.7",
        "@nrwl/workspace": "12.5.7",
        "@testing-library/react": "11.2.6",
        "@types/history": "^4.7.6",
        "@types/jest": "26.0.8",
        "@types/lodash": "^4.14.154",
        "@types/lodash.groupby": "^4.6.6",
        "@types/node": "14.14.33",
        "@types/react": "17.0.3",
        "@types/react-dom": "17.0.3",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.5",
        "@types/redux-mock-store": "^1.0.2",
        "@typescript-eslint/eslint-plugin": "4.19.0",
        "@typescript-eslint/parser": "4.19.0",
        "babel-jest": "26.2.2",
        "cross-env": "^7.0.2",
        "cypress": "^7.3.0",
        "cz-conventional-changelog": "3.2.0",
        "dotenv": "8.2.0",
        "eslint": "7.22.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "7.23.1",
        "eslint-plugin-react-hooks": "4.2.0",
        "generate-template-files": "^3.0.0",
        "husky": "^4.2.5",
        "jest": "27.0.3",
        "nock": "^12.0.3",
        "node-sass": "^4.14.1",
        "npm-run-all": "4.1.5",
        "precise-commits": "1.0.2",
        "prettier": "^2.3.1",
        "pretty-quick": "^2.0.1",
        "redux-mock-store": "^1.5.4",
        "ts-jest": "27.0.3",
        "ts-node": "~9.1.1",
        "typescript": "~4.2.4",
        "validate-commit-msg": "^2.14.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn precommit-lint",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "post-commit": "git update-index -g"
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        },
        "validate-commit-msg": {
            "types": [
                "feat",
                "fix",
                "docs",
                "style",
                "refactor",
                "perf",
                "test",
                "chore",
                "revert",
                "sos"
            ],
            "scope": {
                "required": true,
                "allowed": [
                    "*"
                ],
                "validate": false,
                "multiple": true
            },
            "warnOnFail": false,
            "maxSubjectLength": 150,
            "subjectPattern": ".*",
            "subjectPatternErrorMsg": "",
            "helpMessage": "",
            "autoFix": false
        }
    }
}